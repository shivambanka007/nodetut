<!DOCTYPE html>
<html lang="en">
<link href="css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<link href="css/profile.css" rel="stylesheet">
<link rel="stylesheet" href="css/font-awesome.min.css">
<script src="js/bootstrap.bundle.min.js"></script>
<script src="js/jquery.min.js"></script>

</html>
<div class="container">
<div class="profile-page tx-13">
    <div class="row">
        <div class="col-12 grid-margin">
            <div class="profile-header">
                {{> profileCoverPicPartial}}
                {{> profileNavBarPartial}}
            </div>
        </div>
    </div>
    <div class="row profile-body">
        <!-- left wrapper start -->
        {{> profileLeftAboutPartial}}
        <!-- left wrapper end -->
        <!-- middle wrapper start -->
        <div class="col-md-8 col-xl-6 middle-wrapper">
            <div class="row">
                <div class="col-md-12 grid-margin">
                    <div class="card rounded">
                        {{> profileTimelinePostHeaderPartial}}
                        {{> profileTimelinePostBodyPartial}}
                        {{> profileTimelinePostActionsPartial}}
                    </div>
                </div>
            </div>
        </div>
        <!-- middle wrapper end -->
        <!-- right wrapper start -->
        <div class="d-none d-xl-block col-xl-3 right-wrapper">
            <div class="row">
                {{> profileLatestPhotosPartial}}
                <div class="col-md-12 grid-margin">
                    {{> profileFriendsSuggestionPartial}}
                </div>
            </div>
        </div>
        <!-- right wrapper end -->
    </div>
</div>
</div>